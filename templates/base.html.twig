<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
				{{ site.name }}
			{% endblock %}
		</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="icon" href="{{asset('images/favicone.png')}}"/>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% include "layout/_header.html.twig" %}
		<main class="container" id="main"> {% block body %}{% endblock %}
			</main>
			{% include "layout/_footer.html.twig" %}
		</body>
	</html>
